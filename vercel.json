{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "crons": [
    {
      "path": "/api/market/sync",
      "schedule": "0 0 * * *"
    }
  ],
  "headers": [
    {
      "source": "/api/market/sync",
      "headers": [
        {
          "key": "Authorization",
          "value": "Bearer {{CRON_SECRET}}"
        }
      ]
    }
  ]
}
